{
    "name": "@arpadroid/ui",
    "version": "0.0.1",
    "description": "A UI library of web components",
    "exports": "./dist/arpadroid-ui.js",
    "types": "./dist/@types/types.compiled.d.ts",
    "type": "module",
    "scripts": {
        "build": "arpadroid-build --minify",
        "clean:build": "npm run clean && npm install",
        "clean": "arpadroid-clean",
        "dev": "arpadroid-build --watch --storybook",
        "prepublish": "npm run build",
        "test": "arpadroid-test --storybook --jest",
        "t": "npm run test -- --slim",
        "test:jest": "../module/node_modules/.bin/jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/ui.git"
    },
    "keywords": [
        "web-components",
        "ui",
        "library"
    ],
    "author": "Andres Vaquero @arpadroid <web.andresvaquero@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/ui/issues"
    },
    "homepage": "https://github.com/arpadroid/ui#readme",
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "@babel/preset-env": "^7.28.6",
        "@types/jest": "^30.0.0",
        "babel-jest": "^30.2.0",
        "jest-junit": "^16.0.0"
    },
    "peerDependencies": {
        "@arpadroid/module": "file:../module",
        "@arpadroid/i18n": "file:../i18n",
        "@arpadroid/tools": "file:../tools"
    },
    "dependencies": {
        "material-symbols": "^0.40.2"
    }
}
